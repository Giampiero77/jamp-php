<?xml version="1.0"  encoding="utf-8"?>
<jamp>
	<page typeobj="page" title="JAMP - Modifica connessione" loaddata="false">
		<header typeobj="div" style="height: 30px; width: 100%; background-color: blue">
			<img typeobj="img" align="left" alt="setting.png" src="test/socket.png"/>
			<title typeobj="label" value="" style="color: yellow; font-size: 22px;font-weight: bold" />
		</header>
		<conn typeobj="text" label="\nConnessione:\n" size="30" maxlength="250" keypress="alphanumeric" />
		<type typeobj="dsselect" label="\nTipo\n">
			<options>
				<opt optionvalue="mysql"/>
				<opt optionvalue="mssql"/>
				<opt optionvalue="pgsql"/>
				<opt optionvalue="odbc"/>
				<opt optionvalue="ldap"/>
				<opt optionvalue="xml"/>
				<opt optionvalue="record"/>
				<opt optionvalue="sms"/>
				<opt optionvalue="csv"/>
				<opt optionvalue="filesystem"/>
				<opt optionvalue="ssh"/>
				<opt optionvalue="ftp"/>
			</options>
		</type>
		<host typeobj="text" label="\nHost:\n" minlength="1" maxlength="250" size="50" />	
		<user typeobj="text" label="\nUtente:\n" minlength="1" maxlength="50" size="50" />	
		<pwd typeobj="text" password="true" label="\nPassword:\n" minlength="1" maxlength="50" size="50"/>	
		<port typeobj="text" label="\nPort:\n" minlength="1" maxlength="5" size="5" value="3306" />	
		<button typeobj="button" label="\n\n" value="Conferma" onclick="Confirm()" style="font-weight: bold; font-size: 1.2em; cursor: pointer;"/>	
	</page>
</jamp>
