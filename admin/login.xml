<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE jamp PUBLIC "jamp" "http://jamp.alyx.it/jamp.dtd">
<jamp>
	<page id="page" title="JAMP ADMIN" icon="../../favicon.ico" loaddata="true">
		<ds id="ds1" conn="custom" event="none" />
		<div id="div1" style="height : 150px; top : 50%; position : absolute; width:99%; margin-top : -50px;">
			<table id="table1" align="center">
				<tr id="tr1">
					<td id="td1">
						<img id="img1" src="test/jamp.gif" />
					</td>
					<td id="td2">
						<label id="label1" value="JAMP ADMIN:" style="font-weight: bold; font-size: 2em;" />
						<text id="user" label="\nUtente:\n" maxlength="20" size="20" dsitem="username" />
						<text id="pwd" label="\nPassword:\n" maxlength="20" size="20" password="true" dsitem="password" />
						<button id="label2" label="\n" value="Accedi" onclick="AJAX.login('ds1', 'user', 'pwd');" style="font-weight: bold; font-size: 1.2em; cursor: pointer;" />
					</td>
				</tr>
			</table>
		</div>
	</page>
</jamp>
