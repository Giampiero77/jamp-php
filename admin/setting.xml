<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE jamp PUBLIC "jamp" "./../jamp.dtd">
<jamp>
	<page id="page" title="JAMP - Modifica connessione" loaddata="false">
		<div id="header" style="height: 30px; width: 100%; background-color: blue">
			<img id="img" align="left" alt="setting.png" src="test/setting.png" />
			<label id="title" value="" style="color: yellow; font-size: 22px;font-weight: bold" />
		</div>
		<br id="br" />
		<dsselect id="LANGUAGE" label="LANGUAGE" labelwidth="150px">
			<options>
				<opt optionvalue="IT" />
				<opt optionvalue="EN" />
			</options>
		</dsselect>
		<text id="TIMEZONE" label="TIMEZONE:" minlength="1" maxlength="20" size="50" labelwidth="150px" />
		<text id="TEMPLATE" label="TEMPLATE:" minlength="1" maxlength="20" size="50" labelwidth="150px" />
		<dsselect id="ERROR_LEVEL" label="ERROR_LEVEL" labelwidth="150px">
			<options>
				<opt optiontext="Error" optionvalue="0" />
				<opt optiontext="Debug" optionvalue="1" />
			</options>
		</dsselect>
		<text id="ERROR_REPORTING" label="ERROR REPORTING:" minlength="1" size="50" labelwidth="150px" />
		<dsselect id="COMPRESSHTML" label="COMPRESSHTML" labelwidth="150px">
			<options>
				<opt optionvalue="true" />
				<opt optionvalue="false" />
			</options>
		</dsselect>
		<dsselect id="COMPRESSXML" label="COMPRESSXML" labelwidth="150px">
			<options>
				<opt optionvalue="true" />
				<opt optionvalue="false" />
			</options>
		</dsselect>
		<dsselect id="COMPRESSJS" label="COMPRESSJS" labelwidth="150px">
			<options>
				<opt optionvalue="true" />
				<opt optionvalue="false" />
			</options>
		</dsselect>
		<dsselect id="NOCACHEPHP" label="NOCACHEPHP" labelwidth="150px">
			<options>
				<opt optionvalue="true" />
				<opt optionvalue="false" />
			</options>
		</dsselect>
		<text id="NOCACHEJS" label="NOCACHEJS:" minlength="1" maxlength="250" size="50" labelwidth="150px" />
		<text id="NOCACHECSS" label="NOCACHECSS:" minlength="1" maxlength="250" size="50" labelwidth="150px" />
		<dsselect id="NOUPLOAD" label="NOUPLOAD" labelwidth="150px">
			<options>
				<opt optionvalue="true" />
				<opt optionvalue="false" />
			</options>
		</dsselect>
		<dsselect id="GHOSTDATA" label="GHOSTDATA" labelwidth="150px">
			<options>
				<opt optionvalue="true" />
				<opt optionvalue="false" />
			</options>
		</dsselect>
		<button id="button" label="\n\n" value="Conferma" onclick="Confirm()" style="font-weight: bold; font-size: 1.2em; cursor: pointer;" />
	</page>
</jamp>
