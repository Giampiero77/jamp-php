					<divrag typeobj="div" style="height: 30px">
						<select typeobj="dsselect" label="User" dsobj="ds1" dsitem="User" dsobjlist="ds0" dsitemlist="User" dsitemkeylist="User" />
						<text1 typeobj="text" label="Host" dsobj="ds1" dsitem="Host" size="15" minlength="1" />
					</divrag>
					<f1 typeobj="fieldset" legend="Data" style="width: 140px; height: 115px; float: left">
						<checkbox1 typeobj="checkbox" label="Select" dsobj="ds1" dsitem="Select_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br1 typeobj="br" />
						<checkbox2 typeobj="checkbox" label="Insert" dsobj="ds1" dsitem="Insert_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br2 typeobj="br" />
						<checkbox3 typeobj="checkbox" label="Update" dsobj="ds1" dsitem="Update_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br3 typeobj="br" />
						<checkbox4 typeobj="checkbox" label="Delete" dsobj="ds1" dsitem="Delete_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br4 typeobj="br" />
						<checkbox5 typeobj="checkbox" label="Create" dsobj="ds1" dsitem="Create_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br5 typeobj="br" />
						<checkbox6 typeobj="checkbox" label="Drop" dsobj="ds1" dsitem="Drop_priv" check="Y" uncheck="N" labelwidth="25px" />
					</f1>
					<f2 typeobj="fieldset" legend="Structure" style="width: 140px; height: 115px; float: left;">				
						<checkbox7 typeobj="checkbox" label="Grant" dsobj="ds1" dsitem="Grant_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br7 typeobj="br" />
						<checkbox8 typeobj="checkbox" label="References" dsobj="ds1" dsitem="References_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br8 typeobj="br" />
						<checkbox9 typeobj="checkbox" label="Index" dsobj="ds1" dsitem="Index_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br9 typeobj="br" />
						<checkbox10 typeobj="checkbox" label="Alter" dsobj="ds1" dsitem="Alter_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br10 typeobj="br" />
						<checkbox11 typeobj="checkbox" label="Create tmp table" dsobj="ds1" dsitem="Create_tmp_table_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br11 typeobj="br" />
						<checkbox12 typeobj="checkbox" label="Lock" dsobj="ds1" dsitem="Lock_tables_priv" check="Y" uncheck="N" labelwidth="25px" />
					</f2>
					<f3 typeobj="fieldset" legend="View" style="width: 140px; height: 115px;">
						<checkbox13 typeobj="checkbox" label="Create" dsobj="ds1" dsitem="Create_view_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br12 typeobj="br" />
						<checkbox14 typeobj="checkbox" label="Show" dsobj="ds1" dsitem="Show_view_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br13 typeobj="br" />
						<checkbox15 typeobj="checkbox" label="Create" dsobj="ds1" dsitem="Create_routine_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br14 typeobj="br" />
						<checkbox16 typeobj="checkbox" label="Alter" dsobj="ds1" dsitem="Alter_routine_priv" check="Y" uncheck="N" labelwidth="25px" />
						<br15 typeobj="br" />
						<checkbox17 typeobj="checkbox" label="Execute" dsobj="ds1" dsitem="Execute_priv" check="Y" uncheck="N" labelwidth="25px" />	
					</f3>
				</fpriv>
			</tab1>
			<tab2 typeobj="tab" label="QUERY">
				<divSQL typeobj="div" height="50%" width="autosize">
					<button1 typeobj="button" value="execute" onclick="javascript: execute();" />
					<textSQL typeobj="textarea" label="\n" width="autosize" height="autosize" editor-lang="sql"/>
				</divSQL>
  				<split1 typeobj="splitbar" type="horizontal"/>
  				<divGrid typeobj="div" width="autosize" height="autosize">
					<dsnav2 typeobj="dsnav" dsobj="dsGrid" objprint="gridds2" btNew="false" btSave="false" btCancel="false" btDelete="false" btReload="false" />
 					<gridds2 typeobj="gridds" dsobj="dsGrid" width="autosize" height="autosize" order="true"/>
  				</divGrid>
			</tab2>
			<tab3 typeobj="tab" label="EXPORT">
				<divexptype typeobj="div" style="width:100%; overflow:auto">
					<dsnav3 typeobj="dsnav" dsobj="dsExport" objprint="textSQLExport_code" btNew="false" btSave="false" btCancel="false" btPage="false" btNav="false" btTotal="false" btDelete="false" btReload="false" btPrint="false" style="width: 77px; float: left;">
						<button>
							<bt title="export" onclick="javascript: exportdatabase(false);" image="images/text.png" />
							<bt title="Print" onclick="SYSTEMBROWSER.printContent('textSQLExport_code');" image="images/print.png" />
							<bt title="export to file" onclick="javascript: exportdatabase(true);" image="images/fileexport.png" />
						</button>
					</dsnav3>
					<divExp typeobj="div" style="overflow:auto; margin-top:4px">
						<expnone typeobj="radio" label="none" name="exptype" value="none" align="left" checked="true" />
						<expgz typeobj="radio" label="gzip" name="exptype" value="gz" align="left" />
						<expbz2 typeobj="radio" label="bz2" name="exptype" value="bz2" align="left" />
						<expzip typeobj="radio" label="zip" name="exptype" value="zip" align="left" />
					</divExp>
				</divexptype>
				<divsql typeobj="div" height="50%" width="autosize">
					<textSQLExport typeobj="textarea" dsobj="dsExport" dsitem="SQL" width="autosize" height="autosize" editor-lang="sql"/>
				</divsql>
				<split2 typeobj="splitbar" type="horizontal"/>
				<divexport typeobj="div" height="autosize" width="autosize">
					<createdb typeobj="checkbox" label="Create Database" name="createdb" value="0" check="1" uncheck="0" align="left" />
					<divscroll typeobj="div" height="autosize" width="autosize" style="overflow:auto">
						<fil1 typeobj="fieldset" legend="Tables" style="width: 165px; height: 275px; float: left;"> 
							<select1 typeobj="dsselect" dsobjlist="dsTables" dsitemlist="Name" dsitemkeylist="Name" multiple="multiple" style="width: 165px; height: 200px;" />
							<div1 typeobj="div" style="height: 20px; width: 80px; float: left;">
								<tablestruc typeobj="checkbox" label="Structure" value="1" />
							</div1>
							<div2 typeobj="div"  style="height: 20px; width: 80px;float: left;">
								<tabledata typeobj="checkbox"  label="Data" value="1" />
							</div2>
							<tablestartrow typeobj="text" label="Start Rows" labelwidth="80px" value="0" keypress="number" size="6" minlength="1" />
							<tablelimitrow typeobj="text" label="Max Rows" labelwidth="80px" value="60000" keypress="number" size="6" minlength="1" />
						</fil1>
						<fil2 typeobj="fieldset" legend="Views" style="width: 165px; height: 275px; float: left;"> 	
							<select2 typeobj="dsselect" dsobjlist="dsViews" dsitemlist="Name" dsitemkeylist="Name" multiple="multiple" style="width: 165px; height: 200px;"/>
							<div3 typeobj="div" style="height: 20px; width: 140px; width: 80px;float: left;">
								<viewstruc typeobj="checkbox" label="Structure" value="1" />
							</div3>
							<div4 typeobj="div" style="height: 20px; width: 140px; width: 80px;float: left;">
								<viewdata typeobj="checkbox" label="Data" value="1" />
							</div4>
							<viewstartrow typeobj="text" label="Start Rows" labelwidth="80px" value="0" keypress="number" size="6" minlength="1" />
							<viewlimitrow typeobj="text" label="Max Rows" labelwidth="80px" value="60000" keypress="number" size="6" minlength="1" />
						</fil2>
						<fil3 typeobj="fieldset" legend="Functions/Procedures" style="width: 165px; height: 275px;"> 	
							<select3 typeobj="dsselect" name="functions" dsobjlist="dsFunctions" dsitemlist="Name" dsitemkeylist="Name" multiple="multiple" style="width: 165px; height: 130px;"/>
							<select4 typeobj="dsselect" name="procedures" dsobjlist="dsProcedures" dsitemlist="Name" dsitemkeylist="Name" multiple="multiple" style="width: 165px; height: 130px"/>
						</fil3>
					</divscroll>
				</divexport>
			</tab3>
		</tabs1>
		<exportfile typeobj="form" action="export.php" method="post" target="_blank">
			<datapost typeobj="hidden" name="datapost" value="" />
		</exportfile>
