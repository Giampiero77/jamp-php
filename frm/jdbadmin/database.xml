		<dsExport typeobj="ds" conn="custom" dskey="Name" loadall="false" readonly="true" />
		<tabs1 typeobj="tabs" width="autosize" height="autosize" selected="tab1" align="left">
			<tab1 typeobj="tab" label="DATABASE">
				<fimp typeobj="fieldset" legend="Import Database Object(Tables, Views, Functions, Procedures)" style="width: 550px;height: 30px">
					<importtxt typeobj="text" labelwidth="60px" label=" Import:" fileupload="true" rewrite="true" />
				</fimp>	
				<fpriv typeobj="fieldset" legend="Privileges" style="width: 550px">
